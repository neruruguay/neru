<div class="skt personMarkup">
    <img src="{picture}" alt="{NameText}"/>
    <div class="info">
        <a class="NameLink white normalTip" href="{NameLink}" target="_blank" title="{NameLinkTitle}" ><span>{NameText} SYSTEM</span></a>
        [ProfileLink]
            <a class="ProfileLink white normalTip" title="Profile" href="{ProfileLink}"><span>{ProfileText}</span></a>
        [/ProfileLink]
        [LogoutLink]
            <a class="LogoutLink white normalTip" href="{LogoutLink}"><span>{LogoutText}</span></a>
        [/LogoutLink]
    </div>
</div>